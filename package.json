    "publish:prod": "pnpm build:ts && npm publish"
  },
  "keywords": [
    "bigscreen",
    "大屏适配",
    "vue",
    "scale",
    "适配浏览器缩放",
    "分辨率"
  ],
  "files": [
    "README.md",
    "dist"
  ],
  "author": "meji",
  "license": "ISC",
  "types": "dist/types/lib.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/enhancesd/bigscreen-fit.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@efox/emp": "^2.7.1",
    "@efox/plugin-vue-2": "^1.2.3",
    "vue": "^2.7.16"
  },
  "pnpm": {
    "patchedDependencies": {
      "@efox/emp@2.7.1": "patches/@efox__emp@2.7.1.patch"
    }
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "resize-observer-polyfill": "^1.5.1",
    "vue-demi": "^0.14.7"
